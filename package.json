{
	"name": "new-api",
	"version": "0.0.1",
	"description": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"prebuild": "rimraf dist",
		"prepare": "husky install",
		"build": "nest build",
		"build:prod": "export NODE_OPTIONS=--max_old_space_size=4096 && nest build",
		"format": "prettier --write \"src/**/*.ts\"",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --max-warnings=0",
		"test": "NODE_ENV=test jest --forceExit",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json",
		"migration:create": "typeorm migration:create -n",
		"migration:run": "typeorm migration:run",
		"migration:down": "typeorm migration:revert",
		"seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
		"seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
		"seed:only": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed -s"
	},
	"dependencies": {
		"@nestjs/axios": "^0.0.3",
		"@nestjs/common": "^8.0.0",
		"@nestjs/config": "^1.1.5",
		"@nestjs/core": "^8.0.0",
		"@nestjs/jwt": "^8.0.0",
		"@nestjs/passport": "8.1.0",
		"@nestjs/platform-express": "^8.0.0",
		"@nestjs/platform-socket.io": "^8.4.5",
		"@nestjs/schedule": "^1.0.2",
		"@nestjs/swagger": "^5.0.4",
		"@nestjs/throttler": "^2.0.0",
		"@nestjs/typeorm": "^8.0.2",
		"@nestjs/websockets": "^8.4.5",
		"aws-sdk": "^2.1043.0",
		"bcrypt": "^5.0.1",
		"buffer-image-size": "^0.6.4",
		"class-transformer": "0.4.0",
		"class-validator": "^0.13.1",
		"csv-parse": "^5.0.3",
		"dayjs": "^1.10.7",
		"ejs": "^3.1.6",
		"fs-extra": "^10.0.0",
		"handlebars": "^4.7.7",
		"json2csv": "^5.0.6",
		"lodash": "^4.17.21",
		"md5": "^2.3.0",
		"ms-teams-webhook": "^1.0.4",
		"mysql2": "^2.3.0",
		"passport": "^0.5.0",
		"passport-jwt": "^4.0.0",
		"puppeteer": "^13.1.1",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^7.2.0",
		"sharp": "^0.30.2",
		"socket.io": "^4.1.3",
		"swagger-ui-express": "^4.1.6",
		"typeorm": "^0.2.37",
		"typeorm-seeding": "^1.6.1",
		"uuid": "^8.3.2"
	},
	"devDependencies": {
		"@nestjs/cli": "^8.0.0",
		"@nestjs/schematics": "^8.0.0",
		"@nestjs/testing": "^8.0.0",
		"@trivago/prettier-plugin-sort-imports": "3.1.1",
		"@types/bcryptjs": "^2.4.2",
		"@types/cron": "^1.7.3",
		"@types/express": "^4.17.13",
		"@types/jest": "^26.0.24",
		"@types/multer": "^1.4.7",
		"@types/node": "^16.6.1",
		"@types/puppeteer": "^5.4.4",
		"@types/supertest": "^2.0.11",
		"@types/uuid": "^8.3.3",
		"@typescript-eslint/eslint-plugin": "^4.28.2",
		"@typescript-eslint/parser": "^4.28.2",
		"eslint": "^7.30.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^3.4.0",
		"husky": "^7.0.4",
		"jest": "27.0.6",
		"lint-staged": "^11.2.3",
		"prettier": "^2.3.2",
		"supertest": "^6.1.6",
		"ts-jest": "^27.0.3",
		"ts-loader": "^9.2.3",
		"ts-node": "^10.0.0",
		"tsconfig-paths": "^3.10.1",
		"typescript": "^4.3.5"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	},
	"lint-staged": {
		"!(src/migrations/*)*.ts": [
			"prettier --write",
			"eslint --max-warnings=0"
		]
	}
}
